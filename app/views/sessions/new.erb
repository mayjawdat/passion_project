<h1>Sign in</h1>

<% if @errors %>
  <% @errors.each do |error| %>
    <%= error %>
  <% end  %>
<% end  %>

<form action="/sessions/" method="POST">

<p><label>Email:</label></p>
<input type="text" name="user[email]"></p>

<p><label>Password:</label></p>
<input type="password" name="user[password]"></p>

<br><br>
<input type="submit" name="Log in">

</form>

